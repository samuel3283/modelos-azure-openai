      "cell_type": "markdown",
      "source": [
        "# Embeddings reducidos"
      ],
      "metadata": {
        "id": "-GkAw6evRIBQ"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from openai import AzureOpenAI\n",
        "import numpy as np\n",
        "\n",
        "\n",
        "def normalize_l2(x):\n",
        "    x = np.array(x)\n",
        "    if x.ndim == 1:\n",
        "        norm = np.linalg.norm(x)\n",
        "        if norm == 0:\n",
        "            return x\n",
        "        return x / norm\n",
        "    else:\n",
        "        norm = np.linalg.norm(x, 2, axis=1, keepdims=True)\n",
        "        return np.where(norm == 0, x, x / norm)\n",
        "\n",
        "\n",
        "response = client_azure.embeddings.create(\n",
        "    model=\"text-embedding-3-small\", input=\"Me gusta las peliculas de misterio\", encoding_format=\"float\"\n",
        ")\n",
        "\n",
        "origina_dim = len(response.data[0].embedding)\n",
        "cut_dim = response.data[0].embedding[:256]\n",
        "norm_dim = normalize_l2(cut_dim)\n",
        "\n",
        "print('La dimensionalidad original, es:',origina_dim,' y la nueva dimensionalidad es ahora de:',  len(norm_dim))"
      ],
      "metadata": {
